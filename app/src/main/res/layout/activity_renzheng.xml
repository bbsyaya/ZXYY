<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/homebgc">
    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@color/homebgc">
        <include
            layout="@layout/title_layout" />

    <RelativeLayout
        android:id="@+id/ll_renzhen_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/homebgc"
        android:visibility="visible">
        <RelativeLayout
            android:id="@+id/renzheng_rl_number"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_marginTop="12dp"
            android:background="#ffffff" >

            <EditText
                android:id="@+id/doctor_et_number"
                android:layout_width="match_parent"
                android:layout_height="@dimen/x40"
                android:layout_centerInParent="true"
                android:background="@null"
                android:inputType="phone"
                android:textSize="14sp"
                android:layout_marginLeft="25dp"
                android:hint="@string/qingshuruzhiyeyishi" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_shilitupian"
            android:layout_width="match_parent"
            android:layout_height="190dp"
            android:layout_below="@+id/renzheng_rl_number"
            android:layout_marginTop="12dp"
            android:background="#ffffff">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/shilitupian"
                android:textSize="@dimen/twosize"
                android:textColor="@color/textcolor"
                android:id="@+id/tv_shilitupian"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="12dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/tv_shilitupian">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/small_shili1"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/diyimian"
                        android:textSize="14sp"
                        android:gravity="center"
                        android:layout_marginTop="8dp"/>

                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/small_shili2"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/diermian"
                        android:textSize="14sp"
                        android:gravity="center"
                        android:layout_marginTop="8dp"/>

                </LinearLayout>
            </LinearLayout>

        </RelativeLayout>
        <LinearLayout
            android:id="@+id/ll_second"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rl_shilitupian"
            android:orientation="vertical"
            android:layout_marginTop="12dp"
            android:background="#ffffff">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/shangchuanzhiye"
                android:textSize="@dimen/twosize"
                android:textColor="@color/textcolor"
                android:id="@+id/textView5"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/iv_shangchuan1"
                        android:layout_width="150dp"
                        android:layout_height="120dp"
                        android:scaleType="fitCenter"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/diyimian"
                        android:textSize="14sp"
                        android:gravity="center"
                        android:layout_marginTop="10dp"/>

                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="5dp">
                    <ImageView
                        android:id="@+id/iv_shangchuan2"
                        android:layout_width="150dp"
                        android:layout_height="120dp"
                        android:scaleType="fitCenter"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/diermian"
                        android:textSize="14sp"
                        android:gravity="center"
                        android:layout_marginTop="10dp"/>

                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_xieyi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_second"
            android:layout_marginTop="17dp"
            android:layout_marginLeft="25dp">
            <ImageView
                android:id="@+id/iv_read"
                android:layout_width="14dp"
                android:layout_height="14dp"
                android:background="@drawable/ct_btn_selected_n"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/threesize"
                android:textColor="#787878"
                android:layout_marginLeft="8dp"
                android:text="@string/qingwubiyuedu"/>
            <TextView
                android:id="@+id/tv_xieyi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/threesize"
                android:layout_marginLeft="5dp"
                android:textColor="@color/maincolor"
                android:text="@string/yishixieyi"/>
        </LinearLayout>
        <Button
            android:layout_marginTop="17dp"
            android:id="@+id/btn_commit"
            android:layout_width="match_parent"
            android:layout_height="@dimen/buttonHeight"
            android:text="@string/submit"
            android:textAllCaps="false"
            android:background="@drawable/shape_main_color"
            android:textSize="@dimen/twosize"
            android:textColor="@color/white"
            android:layout_below="@+id/ll_xieyi"
            android:layout_marginLeft="@dimen/mainleft"
            android:layout_marginRight="@dimen/mainright"/>
    </RelativeLayout>

    </LinearLayout>
    </ScrollView>
    <RelativeLayout
        android:id="@+id/rl_touming"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#af000000"
        android:visibility="gone">
        <Button
            android:id="@+id/btn_close"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="13dp"
            android:layout_marginTop="50dp"
            android:alpha="1"
            android:background="@drawable/renzheng_close"/>
        <Button
            android:id="@+id/btn_send"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="28dp"
            android:layout_marginTop="5dp"
            android:text="@string/shangchuantupian"
            android:alpha="1"
            android:textAllCaps="false"
            android:background="@drawable/btn_white_shape"
            android:layout_alignLeft="@+id/textView8"
            android:layout_alignStart="@+id/textView8"
            android:layout_alignRight="@+id/iv_shili"
            android:layout_alignEnd="@+id/iv_shili" />

        <ImageView
            android:layout_width="300dp"
            android:layout_height="200dp"
            android:id="@+id/iv_shili"
            android:alpha="1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_below="@+id/btn_close"
            android:layout_above="@+id/textView7" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/qinganzhaoshilitu"
            android:textColor="#ffffff"
            android:id="@+id/textView7"
            android:alpha="1"
            android:layout_above="@+id/textView8"
            android:layout_alignLeft="@+id/textView8"
            android:layout_alignStart="@+id/textView8" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:text="@string/zhaopianyaosijiao"
            android:textColor="#ffffff"
            android:id="@+id/textView8"
            android:alpha="1"
            android:layout_above="@+id/btn_send"
            android:layout_alignLeft="@+id/iv_shili"
            android:layout_alignStart="@+id/iv_shili" />
    </RelativeLayout>
</FrameLayout>
